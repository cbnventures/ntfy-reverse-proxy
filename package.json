{
  "name": "ntfy-reverse-proxy",
  "displayName": "ntfy Reverse Proxy",
  "version": "1.0.4",
  "description": "A reverse proxy for ntfy running on Cloudflare Workers",
  "exports": "./build/src/index.js",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "wrangler dev",
    "authorize": "wrangler login",
    "deploy-prod": "npm-run-all --sequential deploy-prod:*",
    "deploy-prod:lint": "npx eslint ./",
    "deploy-prod:wrangler": "wrangler deploy --config wrangler-prod.toml",
    "deploy-test": "npm-run-all --sequential deploy-test:*",
    "deploy-test:lint": "npx eslint ./",
    "deploy-test:wrangler": "wrangler deploy --config wrangler-test.toml"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/cbnventures/ntfy-reverse-proxy.git"
  },
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/cbnventures"
    },
    {
      "type": "custom",
      "url": "https://www.cbnventures.io/paypal/"
    }
  ],
  "keywords": [
    "ntfy",
    "push-notification",
    "reverse-proxy",
    "cloudflare"
  ],
  "author": {
    "name": "Jacky Liang",
    "email": "hello@mrjackyliang.com",
    "url": "https://www.mrjackyliang.com"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/cbnventures/ntfy-reverse-proxy/issues"
  },
  "files": [],
  "homepage": "https://github.com/cbnventures/ntfy-reverse-proxy",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "lodash": "4.17.21",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20240117.0",
    "@types/lodash": "4.14.202",
    "@types/node": "20.11.5",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "eslint": "8.56.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "npm-run-all": "4.1.5",
    "typescript": "5.3.3",
    "wrangler": "3.23.0"
  }
}
